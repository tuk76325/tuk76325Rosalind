# The following code when executed substitutes the nucleotide base T for U.
theString = ""
lst = list(theString)
count = 0

for char in theString:
    if char == "T":
        lst[count] = "U"
        count += 1
    else:
        lst[count] = char
        count += 1

transcribedRNA = "".join(lst)
print(transcribedRNA)
