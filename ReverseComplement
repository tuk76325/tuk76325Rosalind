# The following code when executed prints the reverse complement of a given DNA strand.
theString = ""
lst = list(theString)
count = 0

for char in theString:
    if char == "T":
        lst[count] = "A"
        count += 1
    if char == "G":
        lst[count] = "C"
        count += 1
    if char == "C":
        lst[count] = "G"
        count += 1
    if char == "A":
        lst[count] = "T"
        count += 1

lst.reverse()
DNAComplement = "".join(lst)
print(DNAComplement)
