# The following code when executed relays the character count for every nitrogenous base in the given DNA String.
def method1():

    theString = ""
    countA = 0
    countT = 0
    countG = 0
    countC = 0
    for char in theString:
        if char == "A":
            countA += 1
        if char == "T":
            countT += 1
        if char == "C":
            countC += 1
        if char == "G":
            countG += 1
    print("{} {} {} {}".format(countA, countC, countG, countT))
   
